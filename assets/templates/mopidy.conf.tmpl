[core]
cache_dir = /var/cache/mopidy
config_dir = /etc/mopidy/

[audio]
output = alsasink
mixer = alsamixer
mixer_volume = 100

[alsamixer]
card = 0
control= Headphone


[mpd]
hostname = 0.0.0.0

[youtube]
enabled = true

{{if .Spotify}}
[spotify]
enabled = true
username = {{.SpotifyUsername}}
password = {{.SpotifyPassword}}
client_id = {{.SpotifyClientID}}
client_secret = {{.SpotifyClientSecret}}
{{end}}
